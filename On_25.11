using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Parses
{
    class Program
    {
        static void Main(string[] args)
        {
            string[] name = new string[] { "Alice", "Ivan", "John", "Kate", "Something" };
            string[] surName = new string[] { "InChains", "IA", "Newman", "Thomson", "WithSomething" };

            void SortMax()
            {
                Array.Sort(name);

                foreach (var person in name)
                    Console.Write($"{person} ");

                Console.WriteLine("");
            }
            void SortMin()
            {
                Array.Sort(surName);
                Array.Reverse(surName);

                foreach (var person in surName)
                    Console.Write($"{person} ");

                Console.WriteLine("");
            }
            void Mix()
            {
                string[] res = name.Concat(surName).ToArray();
                foreach (var person in res)
                    Console.Write($"{person} ");

                Console.WriteLine("");
            }

            int Dif(int i)
            {
                i -= 123;
                i = Math.Abs(i);
                return i;
            }

            SortMax();
            SortMin();
            Mix();

            int n;

            Console.WriteLine("Введите n");
            n = int.Parse(Console.ReadLine());

            if (n > 123)
                for (int i = 0; i < 3; i++)
                    n = Dif(n);
            else n = Dif(n);

            Console.WriteLine(n);

            Console.ReadLine();
        }
    }
}
